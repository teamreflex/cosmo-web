{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "pnpm run env -- vite dev",
    "build": "pnpm run env -- vite build",
    "start": "pnpm run env -- node .output/server/index.mjs",
    "test": "vitest",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --pretty --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:upgrade": "drizzle-kit up",
    "db:migrate": "pnpm run env -- drizzle-kit migrate",
    "auth:generate": "pnpm run env -- pnpm dlx @better-auth/cli generate --output ../../packages/database/src/auth.ts --y",
    "env": "dotenv -e ../../.env -e ../../.env.local",
    "i18n": "pnpm dlx @inlang/paraglide-js compile --project ./project.inlang --outdir ./src/i18n"
  },
  "dependencies": {
    "@apollo/util": "workspace:*",
    "@aws-sdk/client-ses": "^3.922.0",
    "@fontsource-variable/dm-sans": "^5.2.8",
