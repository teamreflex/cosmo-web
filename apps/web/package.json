    "radix-ui": "^1.4.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.66.0",
    "react-intersection-observer": "^10.0.0",
    "react-qr-code": "^2.0.18",
    "recharts": "^2.15.4",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.4.0",
    "typesense": "^2.1.0",
    "usehooks-ts": "^3.1.1",
