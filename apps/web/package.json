    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@types/umami": "^2.10.1",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@vitejs/plugin-react": "^5.1.0",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "1.0.0-beta.1-ac4ce44",
    "eslint": "9.39.0",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
