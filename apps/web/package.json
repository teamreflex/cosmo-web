{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "env": "bun run --bun --env-file=../../.env --env-file=../../.env.local",
    "dev": "bun run env -- bun run --bun vite",
    "build": "bun run env -- bun run --bun vite build",
    "build:prod": "bun run --bun i18n && bun run --bun typecheck && bun run --bun vite build --mode production",
    "preview": "bun run build && bun run env -- bun run start",
    "start": "bun run --bun --preload ./instrument.ts main.ts",
    "test": "bun test",
    "lint": "oxlint --type-aware",
    "lint:fix": "oxlint --type-aware --fix",
    "typecheck": "tsc --pretty --noEmit",
    "format": "oxfmt .",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:upgrade": "drizzle-kit up",
    "db:migrate": "bun run env -- drizzle-kit migrate",
    "db:seed": "bun run env -- bun run scripts/seed.ts",
    "auth:generate": "bun run env -- bunx @better-auth/cli generate --output ../../packages/database/src/auth.ts --y",
    "i18n": "bunx @inlang/paraglide-js compile --project ./project.inlang --outdir ./src/i18n"
  },
  "dependencies": {
    "@apollo/cosmo": "workspace:*",
    "@apollo/database": "workspace:*",
    "@apollo/util": "workspace:*",
    "@aws-sdk/client-ses": "^3.969.0",
    "@fontsource-variable/dm-sans": "^5.2.8",
    "@hookform/resolvers": "^5.2.2",
    "@sentry/tanstackstart-react": "^10.34.0",
    "@t3-oss/env-core": "^0.13.10",
    "@tabler/icons-react": "^3.36.1",
    "@tanstack/react-query": "^5.90.17",
    "@tanstack/react-router": "1.150.0",
    "@tanstack/react-router-ssr-query": "1.150.0",
    "@tanstack/react-start": "1.150.0",
    "@tanstack/react-virtual": "^3.13.18",
    "better-auth": "1.4.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "drizzle-orm": "1.0.0-beta.10-42c284e",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "fast-average-color-node": "^3.2.0",
    "input-otp": "^1.4.2",
    "jose": "^6.1.3",
    "motion": "^12.26.2",
    "ofetch": "^1.5.1",
    "radix-ui": "^1.4.3",
    "react": "19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "19.2.3",
    "react-error-boundary": "^6.0.3",
    "react-hook-form": "^7.71.1",
    "react-intersection-observer": "^10.0.0",
    "react-player": "^3.4.0",
    "react-qr-code": "^2.0.18",
    "recharts": "^2.15.4",
    "shadcn": "^3.6.3",
    "sonner": "^2.0.7",
    "srvx": "^0.10.0",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "typesense": "^2.1.0",
    "usehooks-ts": "^3.1.1",
    "viem": "^2.44.2",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@apollo/typescript": "workspace:*",
    "@inlang/paraglide-js": "2.9.0",
    "@sentry/vite-plugin": "^4.6.2",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/devtools-vite": "^0.4.1",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/eslint-plugin-router": "^1.141.0",
    "@tanstack/react-devtools": "^0.9.2",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-router-devtools": "^1.150.0",
    "@types/bun": "1.3.6",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "@types/umami": "^2.10.1",
    "@vitejs/plugin-react": "^5.1.2",
    "babel-plugin-react-compiler": "1.0.0",
    "drizzle-kit": "1.0.0-beta.10-42c284e",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-react-google-translate": "^0.1.1",
    "tailwindcss": "^4.1.18",
    "turbo": "^2.7.4",
    "typescript": "5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.4"
  },
  "engines": {
    "bun": ">=1.3.3"
  }
}
